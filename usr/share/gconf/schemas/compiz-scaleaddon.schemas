      <owner>compiz-1</owner>
      <type>int</type>
      <default>0</default>
      <locale name="C">
        <short>Window Layout Mode</short>
        <long>Selects the mode to layout the windows in scale mode (0 = Normal, 1 = Natural)</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/compiz-1/plugins/scaleaddon/screen0/options/natural_precision</key>
      <applyto>/apps/compiz-1/plugins/scaleaddon/screen0/options/natural_precision</applyto>
      <owner>compiz-1</owner>
      <type>float</type>
      <default>2.0</default>
      <locale name="C">
        <short>Precision</short>
        <long>How precisely windows should be pushed apart. Increasing this makes windows that are further apart closer together, but also increases CPU usage (1.0 - 20.0)</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/compiz-1/plugins/scaleaddon/screen0/options/constrain_pull_to_screen</key>
