      <owner>compiz-1</owner>
      <type>int</type>
      <default>1</default>
      <locale name="C">
        <short>Window Title Display</short>
        <long>Window title display in scale mode (0 = No display, 1 = Highlighted window only, 2 = All windows)</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/compiz-1/plugins/scaleaddon/screen0/options/title_bold</key>
